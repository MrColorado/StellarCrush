public class Projectile extends GameObject {
  
  private double dmg;
  private double level;
  
  public Projectile(Vector r, Vector v, double mass, double level) {
    super(r, v, mass);
    this.level = level;
    this.dmg = this.level * 0.5;
  }
  
  public void attack(GameObject o, collection<GameObject>) {
    o.setLife(o.getLife() - this.dmg);
    if (o.getLife() <= 0) {
      
    }
  }
}


